{
  "presets": ["@babel/env", "@babel/typescript"],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    [
      "module-resolver",
      {
        "alias": {
          "@middlewares": "./dist/middlewares",
          "@models": "./dist/db/models",
          "@routes": "./dist/routes",
          "@src": "./dist"
        }
      }
    ]
  ]
}
